class rat
	var a, b: vli
	func mut normalize()
		var gcd = vli.gcd(a, b)
		var rem: vli
		self.a.idiv(gcd, rem)
		self.b.idiv(gcd, rem)
	func mut set(a: int, b: int)
		self.a = vli(a)
		self.b = vli(b)
	func __assign__(i: int)
		self.a = vli(i)
		self.b = vli(1)
	func __assign__(other: rat)
		self.a = other.a
		self.b = other.b
		self.normalize()
	func mut __imul__(other: rat)
		self.a *= other.a
		self.b *= other.b
		self.normalize()
	func mut __idiv__(other: rat)
		self.a *= other.b
		self.b *= other.a
		self.normalize()
	func mut __iadd__(other: rat)
		self.a = self.a * other.b + other.a * self.b
		self.b *= other.b
		self.normalize()
	func mut __isub__(other: rat)
		self.a = self.a * other.b - other.a * self.b
		self.b *= other.b
		self.normalize()
	func __str__() -> string
		return "(" + str(self.a) + " / " + str(self.b) + ")"

func main()
	var r, s: rat
	r.set(3, 2)
	s.set(5, 6)
	r -= s
	print(str(r))
