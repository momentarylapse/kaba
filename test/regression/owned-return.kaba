class X
	func __init__()
		print("X.init")
	func __delete__()
		print("X.del")
	s: string


func f() -> xfer[X]
	var p: owned[X] = new X()
	p.s = "hallo"
	print("---a")
	return give(p)
	var q: owned[X]
	q = give(p)
	print("---b")
	return give(q)

func main()
	let p = f()
	print("bbb")
	print(p.s)
	print(repr(p))

