class A
	func __init__()
		print("A.init")
	func __init__(a: int)
		print("A.init {{a}}")
	func virtual __delete__()
		print("A.del")
	func virtual x()
		print("A.x")

class B extends A
	func override __init__()
		print("B.init")
	func __init__(s: string)
		print("B.init {{s}}")
	func override x()
		print("B.x")

class C extends B
	func override __init__(s: string)
		print("C.init {{s}}")
	func override x()
		print("C.x")

func main()
	var a: owned![A] = new C("test")
	a.x()
