class Base
	var x: int
	func __init__(_x: int)
		x = _x
		print("Base.init_ext")
	func __init__()
		x = 13
		print("Base.init")
	func virtual f()
		print("Base.f")

class Test extends Base
	var y: int
	func __init__(_x: int, _y: int)
		super.__init__(_x)
		print("Test.init")
		y = _y
	func override f()
		print("Test.f")

func main()
	var t = new Test(1, 2)
	print(t.x)
	print(t.y)
	t.f()
	del t
	
