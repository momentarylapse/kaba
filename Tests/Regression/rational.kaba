class rat
	vli a, b
	void normalize()
		vli gcd = self.a.gcd(self.b)
		vli rem
		self.a.idiv(gcd, rem)
		self.b.idiv(gcd, rem)
	void set(int a, int b)
		self.a = a
		self.b = b
	void __assign__(int i)
		self.a = i
		self.b = 1
	void __assign__(rat other)
		self.a = other.a
		self.b = other.b
		self.normalize()
	void __imul__(rat other)
		self.a *= other.a
		self.b *= other.b
		self.normalize()
	void __idiv__(rat other)
		self.a *= other.b
		self.b *= other.a
		self.normalize()
	void __iadd__(rat other)
		self.a = self.a * other.b + other.a * self.b
		self.b *= other.b
		self.normalize()
	void __isub__(rat other)
		self.a = self.a * other.b - other.a * self.b
		self.b *= other.b
		self.normalize()
	string str()
		return "(" + self.a.str() + " / " + self.b.str() + ")"

void main()
	rat r, s
	r.set(3, 2)
	s.set(5, 6)
	r -= s
	print r.str()