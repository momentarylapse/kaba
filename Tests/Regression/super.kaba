class Base
	int x
	void __init__(int _x)
		x = _x
		print("Base.init_ext")
	void __init__()
		x = 13
		print("Base.init")
	virtual void f()
		print("Base.f")

class Test extends Base
	int y
	void __init__(int _x, int _y)
		super.__init__(_x)
		print("Test.init")
		y = _y
	override void f()
		print("Test.f")

void main()
	Test *t = new Test(1, 2)
	print(t.x)
	print(t.y)
	t.f()
	del t
	
