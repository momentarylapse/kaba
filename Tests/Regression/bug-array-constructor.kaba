class Config
	var a, b, c, d: int
	var x: string
	void __init__()
		print("Config.init")
		a = 0
		b = 1
		c = 2
		d = 3

class Base
	var x: string
	void __init__(int i)
		print("Base.init {{i}}")
		x = "hallo"

class Complex extends Base
	var conf: Config[6]
	var a: int
	override void __init__(int i)
		print("Complex.init {{i}}")
		a = 13

void main()
	Complex *c = new Complex(5)
	print(str(*c))
